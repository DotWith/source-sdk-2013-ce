macro OUT_BIN_NAME "server"
macro OUT_BIN_DIR "$SRC_DIR/../game/$GAMENAME/bin"
macro GAMENAME [!$GAMENAME]

include "$SRC_DIR/qpc_scripts/source_dll_base.qpc"

configuration
{
	general
	{
		out_dir "./$CONFIG/$GAMENAME"
		
		include_directories
		{
			"$SRC_DIR/game/server"
			"$SRC_DIR/game/shared"
			"$SRC_DIR/utils/common"
			"$SRC_DIR/game/shared/econ"
			"$SRC_DIR/game/server/NextBot"
		}
	}
	
	compiler
	{
		preprocessor_definitions
		{
			"GAME_DLL"
			"VECTOR"
			"VERSION_SAFE_STEAM_API_INTERFACES"
			"PROTECTED_THINGS_ENABLE"
			"sprintf=use_Q_snprintf_instead_of_sprintf"
			"strncpy=use_Q_strncpy_instead"
			"_snprintf=use_Q_snprintf_instead"
			"fopen=dont_use_fopen"
		}
		
		precompiled_header "use"
		precompiled_header_file "cbase.h"
	}
	
	linker
	{
		libraries
		{
			"winmm.lib"
			"choreoobjects"
			"dmxloader"
			"mathlib"
			"particles"
			"tier2"
			"tier3"
			"steam_api"
		}
	}
}

files
{
	folder "Replay"
	{
		"gamedll_replay.cpp"
		"$SRC_DIR/common/replay/ireplaysessionrecorder.h"
		
		"$SRC_DIR/game/shared/replay_gamestats_shared.cpp"	[$BUILD_REPLAY]
