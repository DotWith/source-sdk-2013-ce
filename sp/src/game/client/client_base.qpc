macro OUT_BIN_NAME "client"
macro OUT_BIN_DIR "$SRC_DIR/../game/$GAMENAME/bin"
macro GAMENAME [!$GAMENAME]

include "$SRC_DIR/qpc_scripts/source_dll_base.qpc"

configuration
{
	general
	{
		out_dir "./$CONFIG/$GAMENAME"
		
		include_directories
		{
			"$SRC_DIR/game/client"
			"$SRC_DIR/game/shared"
			"$SRC_DIR/game/client/game_controls"
		}
	}
	
	compiler
	{
		preprocessor_definitions
		{
			"NO_STRING_T"
			"CLIENT_DLL"
			"VECTOR"
			"VERSION_SAFE_STEAM_API_INTERFACES"
			"PROTECTED_THINGS_ENABLE"
			"strncpy=use_Q_strncpy_instead"
			"_snprintf=use_Q_snprintf_instead"
			"fopen=dont_use_fopen"
		}
		
		precompiled_header "use"
		precompiled_header_file "cbase.h"
	}
	
	linker
	{
		ignore_import_library "true"
		
		libraries
		{
			"winmm.lib"
			"bitmap"
			"bonesetup"
			"choreoobjects"
			"dmxloader"
			"mathlib"
			"matsys_controls"
			"npsclient"
			"particles"
			"raytrace"
			"responserules_runtime"
			"tier2"
			"tier3"
			"vgui_controls"
			"videocfg"
			"vtf"
			"steam_api"
		}
	}
}

